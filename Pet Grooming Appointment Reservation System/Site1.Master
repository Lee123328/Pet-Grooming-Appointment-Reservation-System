                <ul class="menu">
                    <asp:Image ID="Image2" runat="server" ImageUrl="~/Image/logo.png" Width="4%" /><h3 style="color: aqua;">Pet Pamper Pro</h3>
                    <div>
                        <asp:SiteMapDataSource ID="SiteMapDataSource1" runat="server" ShowStartingNode="false" />
                        <asp:Menu ID="Menu1" runat="server" DataSourceID="SiteMapDataSource1" CssClass="menu-items" Orientation="Horizontal">
                        </asp:Menu>
                    </div>
                    <li id="loginMenu" runat="server"><a href="LoginMainChoice.aspx">Login</a></li>
                    <li id="nameMenu" runat="server" visible="false"><a href="#">Welcome,
     <asp:Label ID="UsernameLabel" runat="server" Text=""></asp:Label>!</a></li>

                    <li id="logoutLi" runat="server" style="display: none;">
                        <asp:LinkButton ID="LogoutButton" runat="server" OnClick="LogoutButton_Click">Logout</asp:LinkButton>
                    </li>
                    <div class="box">
                        <input type="text" placeholder="Search">
                        <a href="#">
                            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
                                <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001q.044.06.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1 1 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0" />
                            </svg>
                        </a>
                    </div>
                    <div class="icons">
                        <div class="fas fa-bars" id="menu-btn"></div>
                        <a href="addtocart.aspx">
                            <svg class="w-6 h-6 text-gray-800 dark:text-white" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="currentColor" viewBox="0 0 24 24">
                                <path fill-rule="evenodd" d="M4 4a1 1 0 0 1 1-1h1.5a1 1 0 0 1 .979.796L7.939 6H19a1 1 0 0 1 .979 1.204l-1.25 6a1 1 0 0 1-.979.796H9.605l.208 1H17a3 3 0 1 1-2.83 2h-2.34a3 3 0 1 1-4.009-1.76L5.686 5H5a1 1 0 0 1-1-1Z" clip-rule="evenodd" />
                            </svg>
                        </a>
                        <a href="profile.aspx">
